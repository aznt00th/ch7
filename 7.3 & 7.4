#include <stdio.h>
#include <stdlib.h>
#define maxvals 1000

void mysort(int A[], int n);
void int_swap(int *p1, int *p2);

int
main(int argc, char *argv[]) {
	int n, excess, A[maxvals], x, y, next;
	printf("Enter as many as %d values, ^D to end\n", maxvals);
	n=0; excess=0;
	while (scanf("%d", &next)==1){
		if (n==maxvals) {
			excess = excess+1;
		}else{		
			A[n] = next;
			n+=1;
		}/*n values in array*/
	}printf("%d values read into array\n", n);
	mysort(A, n);
	x=0;
	while (x<n){
		printf("%d",A[x]);
		x+=1;
	}printf("\n");
	x=0;
	printf("Value     Freq\n");
	while (x<n){
		y=1;
		while ((A[x]==A[x+y]) && (x+y<n)){
			y+=1;
			
		}
		printf("%5d %6d\n", A[x], y);
		x+=y;
	}
	return 0;
}

void
mysort(int A[], int n){
	int i, j;
	for (i=1; i<n; i++){
		for (j=i-1; j >=0 && A[j+1]<A[j]; j--) {
			int_swap(&A[j], &A[j+1]);
		}
	}
}
void 
int_swap(int *p1, int *p2){
	int temp=0;
	temp=*p1;
	*p1=*p2;
	*p2=temp;
}
