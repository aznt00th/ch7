#include <stdio.h>
#include <stdlib.h>
#define sizeofA 10
int readarray(int A[]);
void decrease_sort(int A[], int n);
int int_sort2(int *p1, int *p2);

int
main(int argc, char *argv[]) {
	int n, excess, next, A[sizeofA], i;
	printf("Enter as many as %d values, ^D to end\n", sizeofA);
	n=0; excess=0;
	while (scanf("%d", &next)==1){
		if (n==sizeofA){
			excess = excess+1;
		}else {
			A[n] = next;
			n+=1;
		}
	}
	printf("%d values read into array", n);
	if (excess){
		printf(", %d excess values discarded", excess);
	}printf("\n");
	decrease_sort(A, n);
	for (i=0; i<n; i++){
		printf("%d ", A[i]);
	}printf("\n");
	return 0;
}

void
decrease_sort(int A[], int n){
	int i, j;
	for (i=0; i<n; i++){
		for (j=i-1; j>=0 && A[j+1]>A[j]; j--){
			int_sort2(&A[j], &A[j+1]);
		}	
	}
}

int 
int_sort2(int *p1, int *p2){
	int temp=0;
	temp=*p1;
	*p1=*p2;
	*p2=temp;
	return 0;
}
	
